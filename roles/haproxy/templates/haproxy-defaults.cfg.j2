defaults
    log     global
    mode    http
    option  httplog
    option  dontlognull
    timeout connect 5s
    timeout client  60s
    timeout server  60s
    errorfile 400 {{ haproxy_errorfile_dir }}/400.http
    errorfile 403 {{ haproxy_errorfile_dir }}/403.http
    errorfile 408 {{ haproxy_errorfile_dir }}/408.http
    errorfile 500 {{ haproxy_errorfile_dir }}/500.http
    errorfile 502 {{ haproxy_errorfile_dir }}/502.http
    errorfile 503 {{ haproxy_errorfile_dir }}/503.http
    errorfile 504 {{ haproxy_errorfile_dir }}/504.http
